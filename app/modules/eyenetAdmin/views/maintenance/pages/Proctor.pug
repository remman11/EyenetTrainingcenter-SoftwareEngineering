extends ../../../../../templates/eyenetAdmin 

block content
    div.ui.container
        div.padl.padd.padb
            div.ui.segment.eheight
                div.ui.top.attached.large.inverted.green.label Proctors / Professors
                a.button.green.right.floated.ui.basic.button.huge(href="/eyenetAdmin/proctors/new")
                    i.add.icon
                    |&nbsp; Add Proctor
                div.paddd
                table.ui.padded.table
                    thead
                        tr.single.line
                            th Name
                            th.center.aligned.four.wide Actions
                    each user in users
                        tr
                            td=user.strProctorName
                            td.center.aligned
                                a.item(href=`/eyenetAdmin/proctors/${user.intProctorID}`): i.black.big.edit.icon
                                a.item(href=`/eyenetAdmin/proctors/${user.intProctorID}/delete`): i.black.big.remove.icon
                            script.
                                {
                                    var eo = document.getElementById("main");
                                    var et = document.getElementById("proctor");
                                    var ee = document.getElementById("con1");
                                    ee.classList.add("active")
                                    eo.classList.add("active");
                                    et.classList.add("active");
                                }